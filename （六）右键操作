#常规引入
from selenium import webdriver
import time
#引入鼠标操作
from selenium.webdriver.common.action_chains import ActionChains

driver = webdriver.Firefox()
driver.maximize_window

yunpan = ("https://pan.baidu.com/")
driver.find_element_by_id("TANGRAM__PSP_4__footerULoginBtn").click()
driver.find_element_by_id("TANGRAM__PSP_4__userName").send_keys("xx")
driver.find_element_by_id("TANGRAM__PSP_4__password").send.keys("xx")

right = driver.find_element_by_class_name("io1g5G")
ActionChains(driver).context_click(right).perform()










